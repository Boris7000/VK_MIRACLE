<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="colorLikeButton" format="color"/>
    <attr name="colorOnLikeButton" format="color"/>

    <attr name="colorMessageInContainer" format="color"/>
    <attr name="colorOnMessageIn" format="color"/>
    <attr name="colorMessageInPlaceholder" format="color"/>
    <attr name="colorMessageInLink" format="color"/>
    <attr name="colorMessageInStroke" format="color"/>
    <attr name="colorMessageOutContainer" format="color"/>
    <attr name="colorOnMessageOut" format="color"/>
    <attr name="colorMessageOutPlaceholder" format="color"/>
    <attr name="colorMessageOutLink" format="color"/>
    <attr name="colorMessageOutStroke" format="color"/>

    <declare-styleable name="SwitchIconViewV2">
        <attr name="si_tint_color" format="color"/>
        <attr name="si_background_tint_color" format="color"/>
        <attr name="si_animation_duration" format="integer"/>
        <attr name="si_disabled_alpha" format="float"/>
        <attr name="si_disabled_color" format="color"/>
        <attr name="si_disabled_background_color" format="color"/>
        <attr name="si_enabled" format="boolean"/>
        <attr name="si_no_dash" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MiracleRadioView">
        <attr name="CompoundButton_checked" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MiracleRadioGroup">
        <attr name="RadioGroup_checkedButton" format="integer"/>
    </declare-styleable>

    <declare-styleable name="PhotoStackedView">
        <attr name="itemsSpacing" format="dimension"/>
    </declare-styleable>

    <attr name="listItemLayout" format="reference"/>

    <declare-styleable name="PhotoGridView">
        <attr name="spacing" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MessageItemView">
        <attr name="asChat" format="boolean"/>
        <attr name="out" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="OneToOneSquareView">
        <attr name="sizeFrom" format="enum">
            <enum name="width" value="0"/>
            <enum name="height" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="MiracleTextView">
        <attr name="highlightColor" format="color"/>
        <attr name="highlightUnderline" format="boolean"/>
        <attr name="enableClicking" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MaskImageView">
        <attr name="maskBitmap" format="reference"/>
    </declare-styleable>

</resources>